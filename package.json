{
  "name": "mini-dm",
  "version": "0.0.1",
  "description": "Light directory manager",
  "keywords": [
    "Identity-Manager"
  ],
  "homepage": "https://github.com/linagora/mini-dm#readme",
  "bugs": {
    "url": "https://github.com/linagora/mini-dm/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/linagora/mini-dm.git"
  },
  "license": "AGPL-3.0",
  "author": "Xavier Guimard <yadd@debian.org>",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/bin/index.js",
      "types": "./dist/src/bin/index.d.ts"
    },
    "hooks": {
      "types": "./dist/src/hooks.d.ts"
    },
    "expressformatedresponses": {
      "import": "./dist/lib/expressFormatedResponses.js",
      "types": "./dist/src/lib/expressFormatedResponses.d.ts"
    },
    "plugin-auth-llng": {
      "import": "./dist/plugins/auth/llng.js",
      "types": "./dist/src/plugins/auth/llng.d.ts"
    },
    "plugin-auth-openidconnect": {
      "import": "./dist/plugins/auth/openidconnect.js",
      "types": "./dist/src/plugins/auth/openidconnect.d.ts"
    },
    "plugin-auth-token": {
      "import": "./dist/plugins/auth/token.js",
      "types": "./dist/src/plugins/auth/token.d.ts"
    },
    "plugin-demo-helloworld": {
      "import": "./dist/plugins/demo/helloworld.js",
      "types": "./dist/src/plugins/demo/helloworld.d.ts"
    },
    "plugin-ldap-externalusersingroups": {
      "import": "./dist/plugins/ldap/externalUsersInGroups.js",
      "types": "./dist/src/plugins/ldap/externalUsersInGroups.d.ts"
    },
    "plugin-ldap-groups": {
      "import": "./dist/plugins/ldap/groups.js",
      "types": "./dist/src/plugins/ldap/groups.d.ts"
    },
    "plugin-ldap-onchange": {
      "import": "./dist/plugins/ldap/onChange.js",
      "types": "./dist/src/plugins/ldap/onChange.d.ts"
    },
    "plugin-ldap-organization": {
      "import": "./dist/plugins/ldap/organization.js",
      "types": "./dist/src/plugins/ldap/organization.d.ts"
    },
    "plugin-ldap-usersflat": {
      "import": "./dist/plugins/ldap/usersFlat.js",
      "types": "./dist/src/plugins/ldap/usersFlat.d.ts"
    },
    "plugin-static": {
      "import": "./dist/plugins/static.js",
      "types": "./dist/src/plugins/static.d.ts"
    },
    "plugin-twake-james": {
      "import": "./dist/plugins/twake/james.js",
      "types": "./dist/src/plugins/twake/james.d.ts"
    },
    "plugin-weblogs": {
      "import": "./dist/plugins/weblogs.js",
      "types": "./dist/src/plugins/weblogs.d.ts"
    },
    "schema-ad-groups": {
      "import": "./static/schemas/ad/groups.json",
      "require": "./static/schemas/ad/groups.json"
    },
    "schema-ad-organizations": {
      "import": "./static/schemas/ad/organizations.json",
      "require": "./static/schemas/ad/organizations.json"
    },
    "schema-ad-users": {
      "import": "./static/schemas/ad/users.json",
      "require": "./static/schemas/ad/users.json"
    },
    "schema-standard-groups": {
      "import": "./static/schemas/standard/groups.json",
      "require": "./static/schemas/standard/groups.json"
    },
    "schema-standard-organizations": {
      "import": "./static/schemas/standard/organizations.json",
      "require": "./static/schemas/standard/organizations.json"
    },
    "schema-standard-users": {
      "import": "./static/schemas/standard/users.json",
      "require": "./static/schemas/standard/users.json"
    },
    "schema-twake-groups": {
      "import": "./static/schemas/twake/groups.json",
      "require": "./static/schemas/twake/groups.json"
    },
    "schema-twake-organizations": {
      "import": "./static/schemas/twake/organizations.json",
      "require": "./static/schemas/twake/organizations.json"
    },
    "schema-twake-users": {
      "import": "./static/schemas/twake/users.json",
      "require": "./static/schemas/twake/users.json"
    }
  },
  "main": "dist/bin/index.mjs",
  "bin": {
    "mini-dm": "bin/index.mjs"
  },
  "files": [
    "bin",
    "dist",
    "static",
    "LICENSE",
    "package.json"
  ],
  "scripts": {
    "build": "npm run build:prod",
    "build:dev": "./.dev.mk builddev",
    "build:docker": "./.dev.mk builddocker",
    "build:dockerfile": "./.dev.mk Dockerfile",
    "build:prod": "npm run clean && npm run build:dockerfile && NODE_ENV=production rollup -c",
    "build:watch": "rollup -c --watch",
    "check": "npm run lint && npm run format:check",
    "clean": "rimraf dist/*",
    "dev": "npm run build:dev && npm start",
    "fix": "npm run lint:fix && npm run format:fix",
    "format:check": "prettier --check .",
    "format:check-diff": "prettier --check $(git diff $(git rev-parse --abbrev-ref --symbolic-full-name @{u}) --name-status|grep -E '^M|A'|cut -f2)",
    "format:fix": "prettier --write .",
    "format:fix-diff": "prettier --write $(git checkout|grep -E '^(M|A)'|cut -f2)",
    "lint": "eslint \"src/**/*.{ts,tsx,js,jsx}\"",
    "lint:fix": "eslint \"src/**/*.{ts,tsx,js,jsx}\" --fix",
    "start": "./dist/bin/server.js",
    "start:dev": "./.dev.mk start",
    "test": "tsc --noEmit && mocha",
    "test:dev": "./.dev.mk test",
    "test:one": "mocha --config .mocharc-one.json"
  },
  "dependencies": {
    "body-parser": "^2.2.0",
    "express": "^5.1.0",
    "ldapts": "^8.0.9",
    "node-fetch": "^3.3.2",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.4",
    "@types/chai": "^5.2.2",
    "@types/express": "^5.0.3",
    "@types/mocha": "^10.0.10",
    "@types/node": "^24.5.1",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "chai": "^6.0.1",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.5.4",
    "fast-glob": "^3.3.3",
    "mocha": "^11.7.2",
    "nock": "^14.0.10",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "rollup": "^4.50.2",
    "sort-package-json": "^3.4.0",
    "supertest": "^7.1.4",
    "tslib": "^2.8.1",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2"
  },
  "optionalDependencies": {
    "express-openid-connect": "^2.19.2",
    "lemonldap-ng-handler": "^0.7.3",
    "re2": "^1.22.1"
  }
}
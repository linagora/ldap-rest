{
  "attributes": {
    "objectClass": {
      "type": "array",
      "items": {
        "type": "string",
        "test": "^[a-zA-Z][a-zA-Z0-9-]*$"
      },
      "default": ["top", "groupOfNames", "twakeStaticGroup"],
      "required": true
    },
    "cn": {
      "type": "string",
      "test": "^[a-zA-Z][a-zA-Z0-9-]{0,254}$",
      "required": true
    },
    "description": {
      "type": "string",
      "test": "^[a-zA-Z][a-zA-Z0-9-]{0,254}$",
      "required": false
    },
    "mail": {
      "type": "string",
      "test": "^[^@\\s]+@[^@\\s]+\\.[^@\\s]+$",
      "required": false
    },
    "member": {
      "type": "array",
      "items": {
        "type": "string",
        "test": "^[a-zA-Z]+=[a-zA-Z][a-zA-Z0-9-]*,__LDAP_BASE__$"
      },
      "required": false
    },
    "owner": {
      "type": "array",
      "items": {
        "type": "string",
        "test": "^[a-zA-Z]+=[a-zA-Z][a-zA-Z0-9-]*,__LDAP_BASE__$"
      },
      "required": false
    },
    "twakeDepartmentLink": {
      "type": "string",
      "test": "^[a-zA-Z]+=[a-zA-Z][a-zA-Z0-9-]*,",
      "required": true
    },
    "twakeDepartmentPath": {
      "type": "array",
      "items": {
        "type": "string",
        "test": "^[a-zA-Z][a-zA-Z0-9-]*$"
      },
      "required": true
    }
  }
}
